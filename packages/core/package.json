{
    "name": "@clregions/core",
    "version": "0.1.7",
    "description": "",
    "main": "dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "dist/index.mjs",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist/*"
    ],
    "repository": "github:piperubio/clregions",
    "keywords": [
        "cl",
        "chile",
        "regions",
        "provinces",
        "communes",
        "regiones",
        "provincias",
        "comunas",
        "npm",
        "typescript",
        "javascript",
        "ts",
        "js"
    ],
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "format": [
            "esm",
            "cjs"
        ],
        "outDir": "dist",
        "dts": true,
        "clean": true,
        "sourceMap": true
    },
    "scripts": {
        "generate": "tsx ./scripts/generate.mts",
        "format": "biome format --write src/*.ts",
        "check": "biome check  src/*.ts",
        "lint": "tsc",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "build": "tsup",
        "ci": "npm run format && npm run check && npm run lint && npm run build && npm run check-exports",
        "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm --profile node16"
    },
    "license": "MIT",
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.17.3",
        "@clregions/tsconfig": "workspace:*",
        "@types/node": "^22.13.4",
        "@vitest/coverage-c8": "^0.33.0",
        "@vitest/coverage-istanbul": "3.0.6",
        "@vitest/coverage-v8": "3.0.6",
        "tsup": "^8.3.6",
        "tsx": "^4.19.3",
        "typescript": "^5.7.3",
        "vitest": "^3.0.6",
        "@vitest/ui": "^3.0.6"
    },
    "dependencies": {
        "@clregions/data": "1.0.0-next.4"
    }
}