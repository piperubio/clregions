{
  "name": "@clregions/data",
  "version": "1.0.0-next.5",
  "description": "Paquete de datos para regiones de Chile, proporcionando acceso a informaci√≥n estructurada en formato JSON, CSV y SQL",
  "type": "module",
  "main": "./dist/object/index.cjs",
  "module": "./dist/object/index.js",
  "types": "./dist/object/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/object/index.js",
      "require": "./dist/object/index.cjs",
      "types": "./dist/object/index.d.ts"
    },
    "./object": {
      "import": "./dist/object/index.js",
      "require": "./dist/object/index.cjs",
      "types": "./dist/object/index.d.ts"
    },
    "./array": {
      "import": "./dist/array/index.js",
      "require": "./dist/array/index.cjs",
      "types": "./dist/array/index.d.ts"
    },
    "./array/communes": {
      "import": "./dist/array/communes.js",
      "require": "./dist/array/communes.cjs",
      "types": "./dist/array/communes.d.ts"
    },
    "./array/provinces": {
      "import": "./dist/array/provinces.js",
      "require": "./dist/array/provinces.cjs",
      "types": "./dist/array/provinces.d.ts"
    },
    "./array/regions": {
      "import": "./dist/array/regions.js",
      "require": "./dist/array/regions.cjs",
      "types": "./dist/array/regions.d.ts"
    }
  },
  "homepage": "https://github.com/piperubio/clregions",
  "bugs": {
    "url": "https://github.com/piperubio/clregions/issues"
  },
  "repository": "github:piperubio/clregions",
  "scripts": {
    "build": "tsup",
    "format": "biome format --write ts/**/*.ts",
    "check": "biome check  ts/**/*.ts",
    "lint": "tsc",
    "clean": "rimraf dist",
    "ci": "npm run clean && npm run format && npm run check && npm run lint && npm run build && npm run check-exports",
    "check-exports": "attw --pack . --profile node16"
  },
  "keywords": [
    "cl",
    "chile",
    "regions",
    "provinces",
    "communes",
    "regiones",
    "provincias",
    "comunas",
    "npm",
    "typescript",
    "javascript",
    "ts",
    "js",
    "csv",
    "json",
    "sql"
  ],
  "author": "piperubio",
  "license": "MIT",
  "files": ["dist/*"],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@clregions/tsconfig": "workspace:*",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}
